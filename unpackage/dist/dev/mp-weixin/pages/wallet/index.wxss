.content {
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
}
.bottom {
  width: 690rpx;
  height: 93rpx;
  background-color: #ffffff;
  box-shadow: 1rpx 1rpx 6rpx 0rpx rgba(160, 160, 160, 0.55);
  border-radius: 10rpx;
  margin: 0 auto;
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  line-height: 93rpx;
  letter-spacing: 1rpx;
  color: #333333;
}
.wallet {
  width: 690rpx;
  box-shadow: 1rpx 1rpx 6rpx 0rpx rgba(160, 160, 160, 0.55);
  border-radius: 10rpx;
  background-color: #FFFFFF;
  margin: 20rpx auto;
  padding-bottom: 40rpx;
}
.wallet > view:first-child {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: end;
  -webkit-align-items: flex-end;
          align-items: flex-end;
  padding: 0 16rpx;
  padding-top: 26rpx;
  padding-bottom: 10rpx;
  border-bottom: 2rpx solid #ececec;
}
.wallet > view:first-child > view:first-child {
  font-size: 36rpx;
  font-weight: bold;
  letter-spacing: 1rpx;
  color: #333333;
}
.wallet > view:first-child > view:last-child {
  font-size: 28rpx;
  letter-spacing: 1rpx;
  color: #999999;
}
.wallet > view:nth-child(2) {
  width: 100%;
  margin: 20rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.wallet > view:nth-child(2) > view {
  font-size: 30rpx;
  letter-spacing: 1rpx;
  color: #333333;
  width: 50%;
}
.wallet > view:nth-child(2) > view:first-child {
  text-align: right;
  padding-right: 31rpx;
  border-right: 2rpx solid #b7dff4;
}
.wallet > view:nth-child(2) > view:last-child {
  text-align: left;
  padding-left: 31rpx;
}
.state {
  border-radius: 10rpx;
  position: relative;
  margin: 0 auto;
  width: 690rpx;
  height: 170rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  background-repeat: no-repeat;
}
.state > view:first-child {
  font-family: FZCHSJW--GB1-0;
  font-size: 70rpx;
  position: absolute;
  top: 34rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.state .button {
  position: absolute;
  bottom: 10rpx;
  right: 23rpx;
}
.nav {
  padding: 0 30rpx;
  padding-top: 75rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.nav > view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.nav > view > view {
  margin-left: 25rpx;
}
.nav > view > view > view:first-child {
  font-size: 36rpx;
  font-weight: bold;
  letter-spacing: 1rpx;
  color: #333333;
}
.nav > view > view > view:last-child {
  font-size: 28rpx;
  letter-spacing: 1rpx;
  color: #666666;
}

